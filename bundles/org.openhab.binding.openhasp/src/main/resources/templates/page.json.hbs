//Link Previous page to this one
{{#gt (toInt previousPageNum) 0}} 
    {"page":"{{previousPageNum}}","id":"0", "next":"{{pageNum}}"},
{{/gt}}

//Link page 1 to last page
{"page":"1", "id":"0", prev:{{pageNum}}"}

//Container object
{"page":"{{pageNum}}","id":"{{inc "pageObjId"}}", "bg_grad_dir":"0", "bg_color":"#111", "next":"1", "click":"false", "prev","{{previousPageNum}}"}
            
//Background
{"page":"{{pageNum}}","id":"{{inc "pageObjId"}}","obj":"obj", "x":"0", "y":35,"w":"320", "h":"35", "radius":"0", "bg_grad_dir":"0", "bg_opa":"255", "border_width":"0", "bg_color":"#1c1c1c"}

//Back link
{{#if pageLocRePg}}
{"page":"{{pageNum}}","id":"{{inc "pageObjId"}}","obj":"label","x":"5","y":"35","w":"35","h","30","text","\uE04D","text_font","24","text_color","#FFF","click","true","action","p{{pageLocRePg}}"}
{{/if}}

//Page location
{"page":"{{pageNum}}","id":"{{inc "pageObjId"}}","obj":"label","x": "40","y":35,"w":"280","h":"30","text":"{{pageLoc}}","text_font":"24","mode":"scroll","bg_color":"#1c1c1c"}